{
  "name": "my-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "generate-migrations": "drizzle-kit generate:sqlite --config=./drizzle.config.ts",
    "migrate": "bunx tsx ./scripts/migrate.ts",
    "seed": "bunx tsx ./scripts/seed-db.ts",
    "clear": "bunx tsx ./scripts/clear-db.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.17",
    "drizzle-zod": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-headless-table": "^0.18.2",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@libsql/client": "^0.6.0",
    "bits-ui": "^0.21.4",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.30.9",
    "formsnap": "^1.0.0",
    "lucide-svelte": "^0.372.0",
    "mode-watcher": "^0.3.0",
    "slugify": "^1.6.6",
    "svelte-sonner": "^0.3.22",
    "sveltekit-superforms": "^2.12.5",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.0"
  }
}
